{
  "name": "@kenny-assistant/backend",
  "version": "0.0.0",
  "description": "Backend API for Kenny's Personal Assistant",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc -b",
    "start": "node dist/server.js",
    "test": "jest",
    "test:unit": "jest --testPathPattern=unit",
    "test:integration": "jest --testPathPattern=integration",
    "test:intent": "tsx src/scripts/test-intent-with-logging.ts",
    "test:intent:quick": "tsx src/scripts/test-intent-with-logging.ts quick",
    "test:intent:generate": "tsx src/scripts/generate-test-data.ts",
    "test:ollama": "tsx src/scripts/test-ollama-intent.ts",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "db:setup": "tsx src/db/setup-database-automated.ts",
    "db:check": "tsx src/db/check-tables.ts",
    "db:migrate": "cd ../.. && supabase db push",
    "db:reset": "cd ../.. && supabase db reset",
    "db:manual": "bash ../../setup-db.sh",
    "setup": "npm run db:check",
    "context": "tsx src/scripts/manage-context.ts",
    "context:show": "tsx src/scripts/manage-context.ts show",
    "context:clear": "tsx src/scripts/manage-context.ts clear",
    "context:auto": "tsx src/scripts/auto-context.ts capture",
    "context:instructions": "tsx src/scripts/auto-context.ts instructions",
    "context:startup": "tsx src/scripts/auto-context.ts startup"
  },
  "dependencies": {
    "@fastify/cors": "^9.0.1",
    "@fastify/helmet": "^11.1.1",
    "@fastify/multipart": "^7.7.3",
    "@fastify/rate-limit": "^9.1.0",
    "@fastify/swagger": "^8.12.1",
    "@fastify/swagger-ui": "^2.0.1",
    "@supabase/supabase-js": "^2.39.1",
    "chrono-node": "^2.8.4",
    "csv-parse": "^6.1.0",
    "dotenv": "^16.4.7",
    "fastify": "^4.25.2",
    "fastify-plugin": "^4.5.1",
    "langchain": "^0.0.212",
    "pino": "^8.17.2",
    "pino-pretty": "^10.3.1",
    "wink-eng-lite-web-model": "^1.8.1",
    "wink-nlp": "^2.4.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20.10.5",
    "@types/supertest": "^6.0.3",
    "supertest": "^7.1.4",
    "tsx": "^4.7.0"
  }
}
